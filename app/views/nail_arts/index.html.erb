<p id="notice"><%= notice %></p>

<h1>Nail Arts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nail_arts.each do |nail_art| %>
      <tr>
        <td><%= link_to 'Show', nail_art %></td>
        <td><%= link_to 'Edit', edit_nail_art_path(nail_art) %></td>
        <td><%= link_to 'Destroy', nail_art, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nail Art', new_nail_art_path %>
